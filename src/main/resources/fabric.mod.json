{
	"schemaVersion": 1,
	"id": "journeycreative",
	"version": "${version}",
	"name": "journeycreative",
	"description": "Minecraft mod that adds an incremental creative mode based on Terraria's Journey Mode. ",
	"authors": [
		"Abdullah Masud"
	],
	"contact": {
		"homepage": "https://modrinth.com/project/journey-creative/",
		"sources": "https://github.com/abmasud1214/journeycreative",
		"issues": "https://github.com/abmasud1214/journeycreative/issues"
	},
	"license": "MIT",
	"icon": "assets/journeycreative/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"mod.journeycreative.Journeycreative"
		],
		"client": [
			"mod.journeycreative.JourneycreativeClient"
		]
	},
	"mixins": [
		"journeycreative.mixins.json",
		{
			"config": "journeycreative.client.mixins.json",
			"environment": "client"
		}
	],
	"client": [
		"journeycreative.mixin.InventoryScreenMixin"
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": ["1.21.2","1.21.3"],
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"accessWidener": "journeycreative.accesswidener"
}